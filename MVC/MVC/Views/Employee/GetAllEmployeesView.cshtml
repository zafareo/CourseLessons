@model Employee

@{
    ViewData["Title"] = "All employees";
}
<h1> Employee </h1>

<table class="table">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Email</th>
        </tr>
    </thead>
    <tbody>
        @{
            foreach (var item in Model.employees)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td>@item.Email</td>
                    <td> <a href="@Url.Action("GetEmployeeById", "Employee",new{id= item.Id})">Get employees</a></td>
                </tr>
            }
        }
    </tbody>
</table>

